{
  "AllowedHosts": "*",
  "CorsPolicy": {
    "Origins": [
      "http://localhost:5173",
      "http://localhost:8081",
      "http://94.180.174.211:5173",
      "http://94.180.174.211:8081"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Destructure": [
      {
        "Args": {
          "maximumDestructuringDepth": 4
        },
        "Name": "ToMaximumDepth"
      },
      {
        "Args": {
          "maximumStringLength": 100
        },
        "Name": "ToMaximumStringLength"
      },
      {
        "Args": {
          "maximumCollectionCount": 10
        },
        "Name": "ToMaximumCollectionCount"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "Sample"
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AWS": {
    "Profile": "default",
    "BucketName": "supportChatBucket",
    "AccessKey": "",
    "SecretKey": "",
    "ServiceURL": ""
  }
}
