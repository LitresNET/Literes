{
  "AllowedHosts": "*",
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}",
      "RedirectUri": "/api/signin/callback-google"
    },
    "Jwt": {
      "SecurityKey": "${JWT_SECURITY_KEY}"
    }
  },
  "CorsPolicy": {
    "Origins": [
      "http://localhost:5173",
      "http://localhost:8081",
      "http://94.180.174.211:5173",
      "http://94.180.174.211:8081"
    ]
  },
  "Database": {
    "ConnectionString": "${DB_CONNECTION_STRING}",
    "HangfireConnectionString": "${HANGFIRE_CONNECTION_STRING}",
    "TestDbConnectionString": "${TEST_DB_CONNECTION_STRING}"
  },
  "ExternalServices": {
    "PaymentServiceUrl": "${PAYMENT_SERVICE_URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Destructure": [
      {
        "Args": {
          "maximumDestructuringDepth": 4
        },
        "Name": "ToMaximumDepth"
      },
      {
        "Args": {
          "maximumStringLength": 100
        },
        "Name": "ToMaximumStringLength"
      },
      {
        "Args": {
          "maximumCollectionCount": 10
        },
        "Name": "ToMaximumCollectionCount"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "Sample"
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
